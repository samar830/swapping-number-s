#include<stdio.h>
int main(){
	int a,b;
	printf("Enter the value of a\n");
	scanf("%d",&a);
	printf("Enter the value of b\n");
	scanf("%d",&b);
	printf("After swapping ,the value of a and b is %d and %d\n",a,b);
	a = a+b;
	b = a-b;
	a = a-b;
	printf("Before swapping ,the value of a and b is %d and %d",a,b);
return 0;
}
